open System

let rec f a i max n =
    if (i<n) then
        if (a[i]>max) then
            let Max = a[i]
            let i1 = i+1
            f a i1 Max n
        else 
            let Max = max
            let i1 = i+1
            f a i1 Max n
    else max

[<EntryPoint>]
let main argv =
    Console.WriteLine("Введите индекс: ")
    let x = int(Console.ReadLine())
    let a = [|1;2;3;4;5;6;7|]
    
    let n = a.Length
    
    if (a[x]==f a 0 -100 n) then
        printfn "True"
    else printfn "False"
    0
